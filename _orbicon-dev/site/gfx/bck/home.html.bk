<script type="text/javascript">//<![CDATA[

	var ticker_once = false;

	function ticker()
	{
		if(!ticker_once) {		
			__ticker_content = _inv_ticker_content;		
			startticker();
			scrollticker();
			ticker_once = true;
		}
	}

    (function() {
        var tabViewA = new YAHOO.widget.TabView('tabsWrapper');
        var tabViewB = new YAHOO.widget.TabView('tListaAnketa');
        
	var tab0A = tabViewA.getTab(0); 
	var tab2A = tabViewA.getTab(2); 
	tab0A.addListener('click', mysifr); 
	tab2A.addListener('click', mysifr); 

	var tab0B = tabViewB.getTab(0); 
	tab0B.addListener('click', ticker);
	
	//set the active Tab to the cookie value, if present:
	if(YAHOO.util.Cookie.get("tabsWrapper")) {
		tabViewA.set("activeTab", tabViewA.getTab(YAHOO.util.Cookie.get("tabsWrapper")));
	};

	//set the active Tab to the cookie value, if present:
	if(YAHOO.util.Cookie.get("tListaAnketa")) {
		tabViewB.set("activeTab", tabViewB.getTab(YAHOO.util.Cookie.get("tListaAnketa")));
	};

	
	//when a Tab changes, set the cookie:
	tabViewA.on("activeTabChange", function(o) {
		YAHOO.util.Cookie.set("tabsWrapper", this.getTabIndex(o.newValue));
	});
	
	//when a Tab changes, set the cookie:
	tabViewB.on("activeTabChange", function(o) {
		YAHOO.util.Cookie.set("tListaAnketa", this.getTabIndex(o.newValue));
	});

	if((YAHOO.util.Cookie.get("tListaAnketa") == 0) || !YAHOO.util.Cookie.get("tListaAnketa")) {
		ticker();
	}
	
    })();


//]]></script>       

