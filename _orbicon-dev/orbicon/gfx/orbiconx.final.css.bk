body{font:13px/1.22 arial,helvetica,clean,sans-serif;*font-size:small;*font:x-small;}table{font-size:inherit;font:100%;}pre,code,kbd,samp,tt{font-family:monospace;*font-size:108%;line-height:99%;}a img{color:#ffffff;}@media all{a img{color:inherit;border:none;}}h1,h2,h3,h4,h5,h6{font-size:100%;}acronym,abbr{cursor:help;border-bottom-width:1px;border-bottom-style:dotted;}body,div,td,th,h2,h3,h4{voice-family:inherit;}body{margin:0;}p{line-height:140%;}h1,h2{font-weight:bold;}h1{font-size:180%;margin:0;}h2{font-size:140%;padding-bottom:2px;margin:0;}.news_box{padding-bottom:3px;width:100%;float:left;display:inline;position:relative;}div[class="news_box"]{clear:both;}.news_box_preview{float:left;}.s,.h{height:100%;}.s{display:block;}.h{display:none;speak:none;}.news_cat_box{padding-bottom:3px;width:100%;float:left;display:inline;position:relative;}div[class="news_cat_box"]{clear:both;}.news_cat_box_preview{float:left;}ul.horizontal_menu{width:100%;float:left;display:inline;position:relative;}li.h_menu_tab{float:left;}li.h_menu_tab a{float:left;display:block;}.breadcrumbs{width:100%;margin:0;padding:0;}.attila_p1{margin:0;padding:0;font-weight:bold;}.attila_p2{margin:0;padding:0;font-size:90%;}.attila_p3{margin:0;padding:0 0 1em 0;font-size:90%;}.red{color:red;}.green{color:green;}.blue{color:blue;}.yellow{color:yellow;}.bold{font-weight:bold;}.italic{font-style:italic;}.underline{text-decoration:underline;}.orbx_poll_item_title{font-size:85%;margin:0;padding:0;}.orbx_poll_item_bar{font-size:85%;margin:0;padding:0;line-height:1em;}.orbx_poll_item_bar img{height:10px;max-width:100px;max-width:60%;margin:0;padding:0;margin-right:3px;vertical-align:middle;}#orbx_search_container{position:absolute;width:150px;}#orbx_search_container .yui-ac-content{position:absolute;width:100%;border:1px solid #404040;background:#ffffff;overflow:hidden;z-index:9050;}#orbx_search_container .yui-ac-shadow{position:absolute;margin:.3em;width:100%;background:#a0a0a0;z-index:9049;}#orbx_search_container ul{padding:5px 0 5px 0;width:100%;background:white;}#orbx_search_container li{padding:0 5px 0 5px;cursor:default;white-space:nowrap;}#orbx_search_container li.yui-ac-highlight{background:#a0a0a0;}@media print{h1,h2,h3,h4,h5,h6{page-break-after:avoid;page-break-inside:avoid;}blockquote,pre{page-break-inside:avoid;}ul,ol,dl{page-break-before:avoid;}}@media aural{h1,h2,h3,h4,h5,h6{voice-family:paul,male;stress:20;richness:90;}h1{pitch:x-low;pitch-range:90;}h2{pitch:x-low;pitch-range:80;}h3{pitch:low;pitch-range:70;}h4{pitch:medium;pitch-range:60;}h5{pitch:medium;pitch-range:50;}h6{pitch:medium;pitch-range:40;}li,dt,dd{pitch:medium;richness:60;}dt{stress:80;}pre,code,tt{pitch:medium;pitch-range:0;stress:0;richness:80;}em{pitch:medium;pitch-range:60;stress:60;richness:50;}strong{pitch:medium;pitch-range:60;stress:90;richness:90;}dfn{pitch:high;pitch-range:60;stress:60;}s,strike{richness:0;}i{pitch:medium;pitch-range:60;stress:60;richness:50;}b{pitch:medium;pitch-range:60;stress:90;richness:90;}u{richness:0;}a:link{voice-family:harry,male;}a:visited{voice-family:betty,female;}a:active{voice-family:betty,female;pitch-range:80;pitch:x-high;}}.rc,.rc_hd,.rc_bd,.rc_ft,.rc_tr,.rc_mr,.rc_br{background:url(top_infobox_bg.png) no-repeat;}.rc_hd,.rc_tr,.rc_ft,.rc_br{font-size:0px;height:5px;overflow:hidden;}.rc_hd{background-position:left top;}.rc_tr{background-position:right top;margin:0 0 0 10px;}.rc_bd{background-position:0 -10px;}.rc_mr{background-position:100% -10px;margin:0 0 0 10px;}.rc_ft{background-position:bottom left;}.rc_br{background-position:bottom right;margin:0 0 0 10px;}#orbicon_top_infobox{position:relative;margin:10px;}#orbicon_top_infobox .rc_mr{min-height:42px;height:3em;}#orbicon_top_infobox p{color:#434343;padding-top:2px;font-size:75%;margin:0;font-weight:bold;}#orbicon_top_infobox a.close{bottom:6px;right:10px;padding:3px 16px 0 0 !important;background:url(gui_icons/delete.png) no-repeat right bottom !important;font-size:67%;color:#979797;text-decoration:underline;}#orbicon_top_infobox .infobox_content{position:absolute;margin:-4px 0 -18px;padding:7px 190px 10px 15px;width:64%;right:100px;text-align:center;}