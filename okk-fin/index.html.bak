    /**
     * Search for states... use $timeout to simulate
     * remote dataservice call.
     */
    function querySearch (query) {
	
	if(query.trim() == '') {
		return null;
	}
	
      var results = query ? $scope.okz.states.filter( createFilterFor(query) ) : $scope.okz.states,
          deferred;
        return results;
      
    }
	
	function querySearchPoslovnice (query) {
	
	if(query.trim() == '') {
		return null;
	}
	
      var results = query ? $scope.okz.poslovnicePopis.filter( createFilterFor(query) ) : $scope.okz.poslovnicePopis,
          deferred;
      
        return results;
      
    }

